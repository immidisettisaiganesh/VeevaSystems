<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests" thread-count="5">

	<!-- SerialExecution of tests by adding parallel at suite we can do parallel 
		execution -->
	<test name="chromeTest">
		<parameter name="browser" value="chrome" />
		<parameter name="url"
			value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
		<classes>
			<class
				name="StartingWithTestNG.DataProviderandPrallelTesting.ParamTest" />
		</classes>
	</test> <!-- Test -->

	<test name="firefoxTest">
		<parameter name="browser" value="firefox" />
		<parameter name="url"
			value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
		<classes>
			<class
				name="StartingWithTestNG.DataProviderandPrallelTesting.ParamTest" />
		</classes>
	</test> <!-- Test -->

	<test name="edgeTest">
		<parameter name="browser" value="edge" />
		<parameter name="url"
			value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login" />
		<classes>
			<class
				name="StartingWithTestNG.DataProviderandPrallelTesting.ParamTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
